{
	"name": "viacode-frontend",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"prepare": "husky",
		"dev": "next dev",
		"build": "next build",
		"preview": "next start",
		"tsc": "tsc",
		"lint": "set TIMING=1 && eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier . -c -u --cache",
		"format:fix": "prettier . -w -u --cache"
	},
	"lint-staged": {
		"*.{ts,tsx}": "bash -c 'tsc -p tsconfig.json'",
		"**/*.{js,jsx,ts,tsx}": "eslint --fix",
		"*": "prettier -w -u --log-level error"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@emotion/cache": "^11.14.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/material": "^7.3.7",
		"@mui/material-nextjs": "^7.3.7",
		"next": "^16.1.6",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^7.2.0",
		"@eslint-react/eslint-plugin": "^2.7.4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"babel-plugin-react-compiler": "1.0.0",
		"eslint": "^9.39.2",
		"eslint-config-next": "16.1.4",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.26",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.8.1",
		"typescript": "^5"
	}
}
