{
	"name": "viatrade-frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"tsc": "tsc",
		"lint": "set TIMING=1 && eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier . -c -u --cache",
		"format:fix": "prettier . -w -u --cache"
	},
	"lint-staged": {
		"*.{ts,tsx}": "bash -c 'tsc -p tsconfig.app.json'",
		"**/*.{js,jsx,ts,tsx}": "eslint --fix",
		"*": "prettier -w -u --log-level error"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/icons-material": "^7.3.7",
		"@mui/material": "^7.3.7",
		"@reduxjs/toolkit": "^2.11.2",
		"react": "^19.2.4",
		"react-dom": "^19.2.0",
		"react-redux": "^9.2.0",
		"react-router": "^7.13.0",
		"valibot": "^1.2.0",
		"valibot-i18n-better-ru": "^1.0.4"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^7.2.0",
		"@eslint-react/eslint-plugin": "^2.7.4",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"babel-plugin-react-compiler": "^1.0.0",
		"eslint": "^9.39.2",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.24",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.8.1",
		"typescript": "~5.9.3",
		"vite": "^7.3.1",
		"vite-bundle-analyzer": "^1.3.2",
		"vite-plugin-pwa": "^1.2.0"
	}
}
